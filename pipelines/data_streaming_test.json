{
  "debug": true,
  "dry_run": true,
  "stop_on_error": false,
  "artifacts_root": "API-test-space/data_output",
  "description": "Test data streaming with indicators and signal generation",
  "steps": [
    {
      "id": "session_init",
      "cap": "session_scan",
      "enabled": true,
      "inputs": {},
      "description": "Initialize session and verify authentication"
    },
    {
      "id": "profile_check",
      "cap": "profile_scan",
      "enabled": true,
      "inputs": {},
      "description": "Scan user profile and account information"
    },
    {
      "id": "timeframe_setup",
      "cap": "topdown_select",
      "enabled": true,
      "inputs": {
        "stack": "1m",
        "delay_ms": 500,
        "save": true,
        "screenshots_subdir": "timeframe_setup"
      },
      "description": "Set up 1-minute timeframe for quantum_flux_1min strategy"
    },
    {
      "id": "asset_selection",
      "cap": "favorite_select",
      "enabled": true,
      "inputs": {
        "min_pct": 85,
        "select": "first"
      },
      "description": "Select high-probability asset for trading"
    },
    {
      "id": "data_streaming",
      "cap": "data_streaming",
      "enabled": true,
      "inputs": {
        "duration_minutes": 3,
        "strategy_file": "strategies/quantum_flux_1min.json",
        "indicators_enabled": true,
        "signal_generation": true,
        "save_session_data": true
      },
      "description": "Stream real-time data with indicators and signal generation"
    },
    {
      "id": "trade_execution",
      "cap": "trade_click_cap",
      "enabled": true,
      "inputs": {
        "side": "BUY",
        "timeout": 30,
        "root": "API-test-space/data_output"
      },
      "description": "Execute demo trade (dry-run mode)"
    }
  ]
}